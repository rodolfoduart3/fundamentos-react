\n        <div>\n            <h2>O número é {props.numero}</h2>\n            <If test={props.numero % 2 === 0}>\n                <span>Par</span>\n            </If>\n            <If test={props.numero % 2 === 1}>\n                <span>Ímpar</span>\n            </If>\n        </div>\n    )\n    \n}","/var/www/html/fundamentos-react/src/components/basicos/If.jsx",["121"],"\nexport default function(props) {\n    if (props.test) {\n        return props.children\n    } else {\n        return false\n    }\n}","/var/www/html/fundamentos-react/src/components/comunicacao/direta/Pai.jsx",["122"],"import React from 'react'\nimport Filho from './Filho'\n\nexport default props =>\n    <div>\n        <Filho {...props}><strong>João</strong></Filho>\n        <Filho sobrenome={props.sobrenome}><strong>Maria</strong></Filho>\n        <Filho sobrenome=\"Silva\"><strong>Pedro</strong></Filho>\n    </div>","/var/www/html/fundamentos-react/src/components/comunicacao/direta/Filho.jsx",["123"],"import React from 'react'\n\nexport default props =>\n    <div>\n        <p>{props.children} {props.sobrenome}</p>\n    </div>","/var/www/html/fundamentos-react/src/components/comunicacao/indireta/Super.jsx",["124"],"import React, { useState } from 'react'\nimport Sub from './Sub'\n\nexport default (props) => {\n\n    const [texto, setTexto] = useState('Valor')\n    const [num, setNum] = useState(0)\n\n    function quandoClicar(valorGerado, texto) {\n        setNum(valorGerado)\n        setTexto(texto)\n    }\n\n    return (\n        <div>\n            <h4>{texto}: {num}</h4>\n            <Sub onClicar={quandoClicar}>Pedro</Sub>\n        </div>\n    )\n\n}","/var/www/html/fundamentos-react/src/components/comunicacao/indireta/Sub.jsx",["125"],"import React from 'react'\n\nexport default props => {\n\n    function acao() {\n        props.onClicar(Math.random(), 'Gerado')\n    }\n\n    return (\n        <div>\n            <button onClick={acao}>Alterar</button>\n        </div>\n    )\n}","/var/www/html/fundamentos-react/src/components/form/Input.jsx",["126"],"import React, { useState } from 'react'\n\nexport default (props) => {\n\n    const [nome, setNome] = useState('Pedro')\n\n    return (\n        <>\n            <h3>{nome}</h3>\n            <input type=\"text\" \n                value={nome}\n                onChange={e => setNome(e.target.value)}\n            />\n        </>\n    )\n}","/var/www/html/fundamentos-react/src/components/contador/Contador.jsx",[],"/var/www/html/fundamentos-react/src/components/contador/Display.jsx",["127"],"import React from 'react'\n\nexport default (props) => {\n    return (\n        <h4>Valor: {props.valor}</h4>\n    )\n}","/var/www/html/fundamentos-react/src/components/contador/PassoForm.jsx",["128"],"import React from 'react';\n\nexport default (props) => {\n    return (\n        <div>\n            <label htmlFor=\"passoInput\">Passo: </label>\n            <input id=\"passoInput\" \n                type=\"number\" \n                value={props.passo} \n                onChange={(e) => props.onPassoChange(+e.target.value)}\n            />\n        </div>\n    )\n}","/var/www/html/fundamentos-react/src/components/contador/Botoes.jsx",["129"],"import React from 'react';\n\nexport default (props) => {\n    return (\n        <div>\n            <button onClick={props.onInc}>+</button>\n            <button onClick={props.onDec}>-</button>\n        </div>\n    )\n}","/var/www/html/fundamentos-react/src/components/mega/Mega.jsx",["130"],"import React, { useState } from 'react'\n\nexport default (props) => {\n\n    const [numeros, setNumeros] = useState(Array(props.qtdeNumero).fill(0))\n\n    function gerarNumerosNaoContido(array) {\n        const min = 1\n        const max = 60\n        const novoNumero = parseInt(Math.random() * (max - min)) + min\n        return array.includes(novoNumero) ? gerarNumerosNaoContido(array) : novoNumero\n    }\n\n    function gerarNumeros() {\n        const novoArray = Array(props.qtdeNumero)\n                            .fill(0)\n                            .reduce((a,e) => {\n                                const novoNumero = gerarNumerosNaoContido(a)\n                                console.log(a, e, novoNumero)\n                                return [...a, novoNumero]\n                            }, [])\n                            .sort((a,b) => a - b)\n        setNumeros(novoArray)\n    }\n\n    return (\n        <>\n            <h3>Mega</h3>\n            <h4>{numeros.join(' ')}</h4>\n            <button onClick={gerarNumeros}>Gerar Números</button>\n        </>\n    )\n}","/var/www/html/fundamentos-react/src/apitests/InfoApi.jsx",["131"],{"ruleId":"132","replacedBy":"133"},{"ruleId":"134","replacedBy":"135"},{"ruleId":"136","severity":1,"message":"137","line":4,"column":1,"nodeType":"138","endLine":16,"endColumn":11},{"ruleId":"136","severity":1,"message":"137","line":19,"column":1,"nodeType":"138","endLine":71,"endColumn":2},{"ruleId":"136","severity":1,"message":"139","line":1,"column":1,"nodeType":"138","endLine":5,"endColumn":2},{"ruleId":"136","severity":1,"message":"137","line":3,"column":1,"nodeType":"138","endLine":9,"endColumn":11},{"ruleId":"136","severity":1,"message":"137","line":3,"column":1,"nodeType":"138","endLine":7,"endColumn":8},{"ruleId":"136","severity":1,"message":"137","line":3,"column":1,"nodeType":"138","endLine":12,"endColumn":2},{"ruleId":"136","severity":1,"message":"137","line":3,"column":1,"nodeType":"138","endLine":12,"endColumn":2},{"ruleId":"136","severity":1,"message":"137","line":5,"column":1,"nodeType":"138","endLine":21,"endColumn":2},{"ruleId":"136","severity":1,"message":"137","line":4,"column":1,"nodeType":"138","endLine":18,"endColumn":2},{"ruleId":"136","severity":1,"message":"140","line":2,"column":1,"nodeType":"138","endLine":8,"endColumn":2},{"ruleId":"136","severity":1,"message":"137","line":4,"column":1,"nodeType":"138","endLine":9,"endColumn":11},{"ruleId":"136","severity":1,"message":"137","line":3,"colu